<div class="container">
  <h1>Add a new student</h1>
  <div class="new_student">
    <%= simple_form_for [@student_list, @student] do |f| %>
      <%= f.input :name %>
      <%= f.button :submit, "Submit", class: "btn btn-success" %>
    <% end %>
    <%= link_to "Back", student_list_path(params[:student_list_id]), class: "btn btn-success" %>
  </div>
</div>
